                  <view class="weui-select">{{pickerSecurity1[Security1Index]}}</view>
                </picker>
              </view>
            </view>
          </view>

          <view class="weui-flex rowBg interval">
            <view class="tipColor">
              <view class="placeholder">是否在单位买公积金:</view>
            </view>
            <view class="weui-flex__item">
              <view class="weui-cell__ft placeholder">
