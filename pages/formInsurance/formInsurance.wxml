                        </picker>
                      </view>
                    </view>
                  </view>

                </block>

                <view class="weui-flex rowBg interval">
                  <view class="tipColor">
                    <view class="placeholder">请问第{{itemCar}}辆在哪一家银行按揭:</view>
                  </view>
                  <view class="weui-flex__item">
                    <input class='placeholder' name="carWhichBank{{itemCar}}" type='text'></input>
                  </view>
                </view>

              </block>

              <view class="weui-flex rowBg interval">
                <view class="tipColor">
                  <view class="placeholder">请问第{{itemCar}}辆有没有购买商业保险:</view>
                </view>
                <view class="weui-flex__item">
                  <view class="weui-cell__ft placeholder">
                    <switch name="isCarInsurance{{itemCar}}" data-judge="isCarInsurance{{itemCar}}" bindchange="switchJudge" />
                  </view>
                </view>
              </view>

              <block wx:if="{{isCarInsurance5 == true}}">
                <view class="weui-flex rowBg interval">
                  <view class="tipColor">
                    <view class="placeholder">请问第{{itemCar}}辆在什么保险公司买:</view>
                  </view>
                  <view class="weui-flex__item">
                    <input class='placeholder' name="carInsuranceCompany{{itemCar}}" type='text'></input>
                  </view>
                </view>

                <view class="weui-flex rowBg interval">
                  <view class="tipColor">
                    <view class="placeholder">请问第{{itemCar}}辆保险费是多少钱:</view>
                  </view>
                  <view class="weui-flex__item">
                    <input class='placeholder' name="CarInsuranceCost{{itemCar}}"    type='digit'></input>
                  </view>
                </view>

              </block>

            </block>

          </block>

        </block>

      </block>


      <view class="weui-flex">

