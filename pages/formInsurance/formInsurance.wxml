                      <switch name="isCarSupply{{itemCar}}" data-judge="isCarSupply{{itemCar}}" bindchange="switchJudge" />
                    </view>
                  </view>
                </view>

                <block wx:if="{{isCarSupply5 == true}}">

                  <view class="weui-flex rowBg     interval">
