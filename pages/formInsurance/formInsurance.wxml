                      <view class="placeholder">请问第{{itemCar}}辆供完多久:</view>
                    </view>
                    <view class="weui-flex__item">
                      <view class='degree'>
                        <picker name="CarSupplyTime{{itemCar}}" data-type="pickerCarSupplyIndex5" bindchange="pickerChange" value="{{pickerCarSupplyIndex5}}" range="{{pickerCarSupply}}">
